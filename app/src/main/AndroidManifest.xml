<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    package="ge.msda.Finalexam">

    <uses-permission android:name="android.permission.INTERNET" />

    <application

        android:allowBackup="true"
        android:fullBackupContent="false"
        android:icon="@mipmap/examteam"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/examteam"

        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name="com.example.finalexam.signtupintent.Intent3"
            android:configChanges="screenSize|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.finalexam.signtupintent.Intent2"
            android:configChanges="screenSize|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.finalexam.signtupintent.Intent1"
            android:configChanges="screenSize|keyboardHidden"
            android:screenOrientation="portrait" />


        <activity
            android:name="com.example.finalexam.UpdateinfoActivity"
            android:configChanges="screenSize|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.finalexam.ForgotPasswordActivity"
            android:configChanges="screenSize|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.finalexam.signtupintent.Intent4"
            android:configChanges="screenSize|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.finalexam.LoginActivity"
            android:configChanges="screenSize|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.finalexam.FirstpageActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>